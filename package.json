    "babel-eslint": "^10.1.0",
    "bcurl": "^0.1.6",
    "bmocha": "^2.1.3",
    "eslint": "^7.1.0",
    "hs-client": "0.0.8",
    "hsd": "github:handshake-org/hsd",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.6.4"
